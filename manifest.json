{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Metalete",
  "description": "Delete all conversations in messenger",
  "version": "1.1",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "icons":				{
	
	"16":				"16x16.png", 
	"32":				"32x32.png", 
	"48":				"48x48.png", 
	"128":				"128x128.png"
},
  "permissions": [
   "activeTab","tabs"
   
   ],
   
     "web_accessible_resources": ["script.js"]
}